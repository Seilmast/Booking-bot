import tkinter as tk
import urllib as url
from selenium import webdriver
import time

driver = webdriver.Firefox()
#Åpner nettsiden til UiT outlook
driver.get('https://mail.uit.no/owa/auth/logon.aspx?replaceCurrent=1&url=https%3a%2f%2fmail.uit.no%2fowa')

#Venter så siden skal få tid til å laste inn
time.sleep(5)

#Trykker på knappen med gitt id
button_element = driver.find_element_by_id('')
button_element.click()

#Lar neste side laste inn
time.sleep(5)
